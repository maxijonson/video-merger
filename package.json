{
    "name": "video-merger",
    "version": "1.0.0",
    "description": "",
    "main": "dist/esm/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/@types/index.d.ts",
    "type": "module",
    "scripts": {
        "start": "pm2-runtime start pm2.config.cjs --env production",
        "dev": "ts-node -P tsconfig.esm.json src/index.ts",
        "prod": "npm run build && npm run start",
        "watch": "nodemon",
        "clean": "rm -rf dist",
        "build": "npm-run-all clean build:esm",
        "build:esm": "tsc -p tsconfig.prod.json && cp .build/package.esm.json dist/esm/package.json",
        "reinstall": "npm run clean && rm -rf node_modules package-lock.json && npm install"
    },
    "exports": {
        ".": "./dist/esm/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maxijonson/video-merger.git"
    },
    "keywords": [],
    "files": [
        "dist"
    ],
    "author": "Tristan Chin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/maxijonson/video-merger/issues"
    },
    "homepage": "https://github.com/maxijonson/video-merger#readme",
    "engines": {
        "node": "16.x"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/lodash": "^4.14.182",
        "@types/multer": "^1.4.7",
        "@types/node-schedule": "^2.1.0",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "5.30.0",
        "@typescript-eslint/parser": "5.30.0",
        "eslint": "8.18.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-typescript": "3.0.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.1.0",
        "nodemon": "^2.0.18",
        "npm-run-all": "4.1.5",
        "prettier": "2.7.1",
        "ts-node": "10.8.1",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "dotenv": "^16.0.1",
        "express": "^5.0.0-beta.1",
        "fs-extra": "^10.1.0",
        "lodash": "^4.17.21",
        "lowdb": "^3.0.0",
        "moment": "^2.29.3",
        "multer": "^1.4.5-lts.1",
        "node-schedule": "^2.1.0",
        "pm2": "^5.2.0",
        "uuid": "^8.3.2"
    }
}
